#!/bin/sh

xrdb ~/.Xresources

setbg &		# Set the background
setxkbmap -layout se -option caps:super && killall xcape 2>/dev/null ; xcape -e 'Super_L=Escape' &
xmodmap -e 'keycode 135 = Super_R' &
xset r rate 300 50 &	# Speed xrate up
xbindkeys &
unclutter &		# Remove mouse when idle
xcompmgr &		# xcompmgr for transparency
mpdupdate &

dunst &
/usr/lib/kdeconnectd &
redshift -r -t 6500:2000 &
profile-loader &
